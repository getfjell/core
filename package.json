{
  "name": "@fjell/core",
  "description": "Core Item and Key Framework for Fjell",
  "version": "4.4.19",
  "keywords": [
    "core",
    "fjell"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "dev": "concurrently \"tsc --noEmit --watch\" \"node esbuild.config.js --watch\"",
    "build": "pnpm run lint && tsc --noEmit && node esbuild.config.js && node generate-types.js",
    "clean": "rimraf dist",
    "test": "pnpm run lint && vitest run --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/docs-template": "link:../fjell-docs-template",
    "@fjell/logging": "^4.4.19",
    "deepmerge": "^4.3.1",
    "luxon": "^3.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@fjell/eslint-config": "^1.0.5",
    "@swc/core": "^1.13.2",
    "@tsconfig/recommended": "^1.0.10",
    "@types/luxon": "^3.6.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.13.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/core.git"
  }
}
